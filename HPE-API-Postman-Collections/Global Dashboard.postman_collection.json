					]
				}
			},
			"response": []
		},
		{
			"name": "Get-X-API-Version",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"var jsonData = JSON.parse(responseBody);",
							"",
							"var xapiversion =  jsonData.currentVersion;",
							"",
							"//Set environment variable",
							"postman.setEnvironmentVariable(\"xapiversion\", xapiversion);",
							"",
							"//Display X-API-version in test results",
							"tests[\"X API version = \" + xapiversion ] = true; "
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "GET",
				"header": [
					{
						"key": "X-API-Version",
						"value": "300"
					},
					{
						"key": "Auth",
						"value": "{{sessionID}}"
					}
				],
				"url": {
					"raw": "https://{{globaldashboard}}/rest/version",
					"protocol": "https",
					"host": [
						"{{globaldashboard}}"
					],
					"path": [
						"rest",
						"version"
					]
				},
				"description": "Login Session"
			},
			"response": []
		},
		{
			"name": "Get-Appliances",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"var jsonData = JSON.parse(responseBody);\r",
							"\r",
							"var appliance =  jsonData.members;\r",
							"var appliancenumber = jsonData.total;\r",
							"var applianceXAPI = jsonData.currentApiVersion;\r",
							"\r",
							"\r",
							"var appl = [];\r",
							"for (i = 0; i < appliancenumber; ++i) {\r",
							"\r",
							"    var appliancename = \"appliance-\" + (i+1);\r",
							"    var applianceid = \"appliance-\" + (i+1) + \"-id\";\r",
							"    var applianceXAPI = \"appliance-\" + (i+1) + \"-XAPI\";\r",
							"\r",
							"    //Set environment variable\r",
							"    postman.setEnvironmentVariable(appliancename, appliance[i].hostname );\r",
							"    tests[appliancename + \" = \" + appliance[i].hostname] = true; \r",
							"\r",
							"    postman.setEnvironmentVariable(applianceid, appliance[i].id );\r",
							"    tests[applianceid + \" = \" + appliance[i].id] = true; \r",
							"\r",
							"    postman.setEnvironmentVariable(applianceXAPI, appliance[i].currentApiVersion );\r",
							"    tests[applianceXAPI + \" = \" + appliance[i].currentApiVersion] = true; \r",
							"    \r",
							"}"
						],
						"type": "text/javascript"
					}
				}
			],
			"protocolProfileBehavior": {
				"disableBodyPruning": true
			},
			"request": {
				"method": "GET",
				"header": [
					{
						"key": "x-api-version",
						"value": "{{xapiversion}}",
						"type": "text"
					},
					{
						"key": "auth",
						"value": "{{GDsessionID}}",
						"type": "text"
					}
				],
				"body": {
					"mode": "raw",
					"raw": ""
				},
				"url": {
					"raw": "https://{{globaldashboard}}/rest/appliances",
					"protocol": "https",
					"host": [
						"{{globaldashboard}}"
					],
					"path": [
						"rest",
						"appliances"
					]
				}
			},
			"response": []
		},
